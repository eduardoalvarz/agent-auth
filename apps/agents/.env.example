TAVILY_API_KEY=...

# To separate your traces from other application
LANGCHAIN_PROJECT=react-agent
LANGCHAIN_API_KEY=...
# LANGCHAIN_TRACING_V2=true


#SUPABASE 
SUPABASE_URL=
# service role key - do not expose to client
SUPABASE_KEY=

# The following depend on your selected configuration

## LLM choice:
ANTHROPIC_API_KEY=....

OPENAI_API_KEY=...

#Optional
FIREWORKS_API_KEY=...

# BigQuery / GCP
# Either set GOOGLE_APPLICATION_CREDENTIALS to a JSON key path,
# or provide explicit service account credentials below.
BQ_PROJECT_ID=coop-query
BQ_DATASET=bridge_coop
BQ_TABLE=B2B_SO

# Explicit credentials (preferred for serverless)
GCP_CLIENT_EMAIL=
# If your private key contains literal \n in the .env file, keep them as \n;
# the code will convert them to newlines at runtime.
GCP_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"

# Alternative: path to JSON key file, used by ADC fallback
GOOGLE_APPLICATION_CREDENTIALS=
